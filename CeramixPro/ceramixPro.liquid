                    <div class="carousel-item">
                        {% if section.settings.image4 != blank -%}
                            {{- section.settings.image4 | image_url:  | image_tag: class: 'd-block w-100', loading: 'lazy' -}}
                        {%- endif %}
                    </div>
                </div>
                <button
                    class="carousel-control-prev"
                    type="button"
                    data-mdb-target="#carouselExampleCaptions"
                    data-mdb-slide="prev"
                >
                    {{ section.settings.button4 }}
                </button>
                <button
