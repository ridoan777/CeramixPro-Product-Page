            <div id="carouselExampleCaptions" class="carousel slide" data-mdb-ride="carousel">
                <div class="carousel-indicators">
                    <button
                        data-mdb-target="#carouselExampleCaptions"
                        data-mdb-slide-to="0"
                        class="active btn btn-primary rounded-circle mt-5"
                        aria-current="true"
                        aria-label="Slide 1"
                        style="background-color: {{ section.settings.button1Backgroundcolor }}; height: {{ section.settings.button1Height }}; width: {{ section.settings.button1Width }}; border: 0px !important;"
                    >
                        {{ section.settings.button1 }}
                    </button>
                    <button
                        data-mdb-target="#carouselExampleCaptions"
                        data-mdb-slide-to="1"
                        class="btn btn-primary rounded-circle mt-5"
                        aria-label="Slide 2"
                        style="background-color: {{ section.settings.button2Backgroundcolor }}; height: {{ section.settings.button2Height }}; width: {{ section.settings.button2Width }}; border: 0px !important;"
                    >
                        {{ section.settings.button2 }}
                    </button>
                    <button
                        data-mdb-target="#carouselExampleCaptions"
                        data-mdb-slide-to="2"
                        class="btn btn-primary rounded-circle mt-5"
                        aria-label="Slide 3"
                        style="background-color: {{ section.settings.button3Backgroundcolor }}; height: {{ section.settings.button3Height }}; width: {{ section.settings.button3Width }}; border: 0px !important;"
                    >
                        {{ section.settings.button3 }}
                    </button>
                </div>
                <div class="heroSliderIndicatorText">Sample For More!</div>

                <div class="carousel-inner">
                    <div class="carousel-item active">
                        {% if section.settings.image2 != blank -%}
                            {{- section.settings.image2 | image_url:  | image_tag: class: 'd-block w-100', loading: 'lazy' -}}
                        {%- endif %}
                    </div>
                    <div class="carousel-item">
                        {% if section.settings.image3 != blank -%}
                            {{- section.settings.image3 | image_url:  | image_tag: class: 'd-block w-100', loading: 'lazy' -}}
                        {%- endif %}
